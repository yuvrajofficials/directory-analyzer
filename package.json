{
  "name": "temporal-continue-as-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "temporal": "temporal server start-dev",
    "src:worker": "npx ts-node src/workers/fs-worker.ts 1",
    "src:client": "npx ts-node src/clients/fs-client.ts 5",
    "source:worker": "npx ts-node source/workers/fs-worker.ts 1",
    "source:client": "npx ts-node source/clients/fs-client.ts 5",
    "debug:worker": "node --inspect-brk -r ts-node/register src/workers/fs-worker.ts",
    "debug:client": "node --inspect-brk -r ts-node/register src/clients/fs-client.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.0",
    "@faker-js/faker": "^10.0.0",
    "@google-cloud/storage": "^7.10.0",
    "@temporalio/activity": "^1.11.8",
    "@temporalio/client": "^1.11.8",
    "@temporalio/worker": "^1.11.8",
    "@temporalio/workflow": "^1.11.8",
    "@tensorflow/tfjs": "^4.22.0",
    "aws-sdk": "^2.1530.0",
    "babel-core": "^6.26.3",
    "cypress": "^13.5.0",
    "eslint": "^9.0.0",
    "express": "^4.18.2",
    "firebase": "^11.0.0",
    "jest": "^29.7.0",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.6.0",
    "mysql": "^2.18.1",
    "nest": "^0.1.0",
    "next": "^14.0.0",
    "onnxruntime-node": "^1.17.0",
    "pg": "^8.16.3",
    "playwright": "^1.45.0",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.32.1",
    "typeorm": "^0.3.17",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
